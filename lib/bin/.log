import '../colors.dart';
import 'Container1.dart';

class Dropdown1 extends StatefulWidget {
  final List<String> array;
  final Color color;
  final String placeholder;
  final String value;
  final double my;
  final Function(String) onhasChanged;
  const Dropdown1({
    Key key,
    this.array,
    this.onhasChanged,
    this.color = accentColor,
    this.placeholder = "Select...",
    this.my = 3,
    this.value,
  }) : super(key: key);

  @override
  State<Dropdown1> createState() => _Dropdown1State(
      array: array, color: color, my: my, placeholder: placeholder);
}

class _Dropdown1State extends State<Dropdown1> {
  String _value;
  final List<String> array;
  final Color color;
  final String placeholder, value;
  final double my;
  final Function(String) onhasChanged;

  _Dropdown1State(
      {this.array = const [],
      this.onhasChanged,
      this.color = accentColor,
      this.placeholder = "Select...",
      this.my = 3,
      this.value});

  DropdownMenuItem<String> buildMenuItem(String item) {
    return DropdownMenuItem(
      child: Text(item),
      value: item,
    );
  }

  @override
  void initState() {
    if (this.value != '') {
      setState(() {
        _value = value;
      });
      print(value);
    }
    super.initState();
  }

  @override
  Widget build(BuildContext context) {
    // print(_value + " is value");
    return Container1(
      my: my,
      opacity: .2,
      height: 50,
      child: DropdownButton(
        underline: Container(),
        hint: Text(placeholder),
        icon: Icon(
          Icons.arrow_drop_down_circle_outlined,
          color: color,
        ),
        iconSize: 20,
        value: _value,
        isExpanded: true,
        items: ((array.map(buildMenuItem)).toList()),
        onChanged: (value) {
          setState(() => this._value = value);
          onhasChanged(value);
        },
      ),
    );
  }
}


